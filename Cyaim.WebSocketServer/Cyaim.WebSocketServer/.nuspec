<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata>
		<id>Cyaim.WebSocketServer</id>
		<version>1.6.1</version>
		<authors>Psyche</authors>
		<owners>Cyaim Studio</owners>
